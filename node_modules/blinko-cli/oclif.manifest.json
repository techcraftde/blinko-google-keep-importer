{
  "commands": {
    "dev:server": {
      "aliases": [],
      "args": {},
      "description": "Start a development server for Blinko plugins with hot-reloading",
      "examples": [
        "$ blinko-cli dev server\nStarting Blinko development server...",
        "$ blinko-cli dev server --ws-port=9000 --http-port=4000\nStarting Blinko development server with custom ports..."
      ],
      "flags": {
        "ws-port": {
          "description": "WebSocket server port",
          "name": "ws-port",
          "default": 8080,
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "http-port": {
          "description": "HTTP documentation server port",
          "name": "http-port",
          "default": 3000,
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "dist-dir": {
          "description": "Directory containing build output",
          "name": "dist-dir",
          "default": "./dist",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "plugin-json": {
          "description": "Path to plugin.json file",
          "name": "plugin-json",
          "default": "./plugin.json",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "vite-command": {
          "description": "Vite command to run",
          "name": "vite-command",
          "default": "vite",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        }
      },
      "hasDynamicHelp": false,
      "hiddenAliases": [],
      "id": "dev:server",
      "pluginAlias": "blinko-cli",
      "pluginName": "blinko-cli",
      "pluginType": "core",
      "strict": true,
      "enableJsonFlag": false,
      "isESM": true,
      "relativePath": [
        "dist",
        "commands",
        "dev",
        "server.js"
      ]
    },
    "release:plugin": {
      "aliases": [],
      "args": {},
      "description": "Release plugin to Blinko marketplace",
      "examples": [
        "$ blinko-cli release plugin\nReleasing plugin from current directory...",
        "$ blinko-cli release plugin --dir ./my-plugin\nReleasing plugin from ./my-plugin directory..."
      ],
      "flags": {
        "dir": {
          "char": "d",
          "description": "Directory containing plugin.json and release folder",
          "name": "dir",
          "default": ".",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        }
      },
      "hasDynamicHelp": false,
      "hiddenAliases": [],
      "id": "release:plugin",
      "pluginAlias": "blinko-cli",
      "pluginName": "blinko-cli",
      "pluginType": "core",
      "strict": true,
      "enableJsonFlag": false,
      "isESM": true,
      "relativePath": [
        "dist",
        "commands",
        "release",
        "plugin.js"
      ]
    }
  },
  "version": "0.0.4"
}